FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      s;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet_s
    {
	type		fixedValue;
	value		uniform 1;
    }

    inlet
    {
	type		fixedValue;
	value		uniform 0;
    }

    up_down
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;
    }

    inner
    {
	type		fixedValue;
	value		uniform 0;
    }

    defaultFaces
    {
        type            empty;
    }
}

